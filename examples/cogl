dna GmoCoglCube BaseDrawable LbaMutogene3D LbaCogl LbaCoglCube
dna GmoCoglLabel BaseDrawable LbaMutogene3D LbaCogl LbaCoglLabel

create LbaCoglWindow w
create GmoCoglCube c
create GmoCoglLabel l

create LbaCoglTexture t
set c.texture t

set c.drawing-scene w
set l.drawing-scene w

set l.x 0.1
set l.y 0.1
set l.text Bombolla
set l.font-size 40
set l.font-name Chilanka

create GmoCoglLabel l2
set l2.drawing-scene w
set l2.x 0.1
set l2.y 0.3
set l2.text is the best shell
set l2.font-size 30
set l2.font-name Ani

call w.open
